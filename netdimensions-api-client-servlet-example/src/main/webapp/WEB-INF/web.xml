<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
  <display-name>NetDimensions Talent Suite API Client Library for Java - Sample Webapp</display-name>
  <filter>
    <filter-name>OAuthFilter</filter-name>
    <filter-class>com.netdimensions.client.servlet.OAuthFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>OAuthFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.netdimensions.client.servlet.OAuthSessionListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>CallbackServlet</servlet-name>
    <servlet-class>com.netdimensions.client.servlet.CallbackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CallbackServlet</servlet-name>
    <url-pattern>/cb</url-pattern>
  </servlet-mapping>
</web-app>
